<script src="~/lib/oidc-client/oidc-client.min.js"></script>
<script>

    var userManger = new Oidc.UserManager({
        userStore: new Oidc.WebStorageStateStore({ store: window.localStorage })  //here we say to Oidc where to save it
    });
    userManger.signinCallback().then(res => {
        console.log(res);
        window.location.href = '/home/index';
    });
</script>

